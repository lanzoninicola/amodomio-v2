    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc",
    "migrate:dev": "npx prisma migrate dev --name",
    "migrate:deploy": "npx prisma migrate deploy",
    "test": "vitest"
